<h1>Listing Users</h1>

<table class="table">
  <tr>
    <th>ID</th>
    <th>User</th>
    <th>Inserted</th>
    <th>Updated</th>
  </tr>
  <%= for user <- @users do %>
    <tr>
      <td><%= user.id %></td>
      <td><%= link "#{user.name}", to: user_path(@conn, :show, user.id) %></td>
      <td><%= user.inserted_at %></td>
      <td><%= user.updated_at %></td>
    </tr>
  <% end %>
</table>
